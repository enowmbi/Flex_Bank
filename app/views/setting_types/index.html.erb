<p id="notice"><%= notice %></p>

<h1>Setting Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setting_types.each do |setting_type| %>
      <tr>
        <td><%= setting_type.name %></td>
        <td><%= setting_type.description %></td>
        <td><%= link_to 'Show', setting_type %></td>
        <td><%= link_to 'Edit', edit_setting_type_path(setting_type) %></td>
        <td><%= link_to 'Destroy', setting_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting Type', new_setting_type_path %>
